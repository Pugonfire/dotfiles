[eyedropper]
command=~/.config/i3blocks/dropper
full_text=Dropper
color=#988e01
instance=rgb

[date]
command=date +"%a %d %b"
interval=30

[time]
command=date +"%T"
interval=1

[rofi]
full_text=
command=rofi -show run
color=#ffffff

[power]
full_text=
command=rofi -show power-menu -modi power-menu:~/.config/rofi/rofi-power-menu
color=#ffffff
